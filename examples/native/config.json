{
  "type": "native",
  "devices": {
    "nexus6p": {}
  },
  "replications": 1,
  "duration": 5000,
  "apps": [
    "com.experimental.rvs960.ioapp"
  ],
  "profilers": {
     "trepn": {
      "sample_interval": 200,
      "data_points": ["battery_power"]
     }
 },
  "scripts": {
    "before_experiment": "Scripts/before_experiment.py",
